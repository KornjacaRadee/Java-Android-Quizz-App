<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#02106C"
    android:layout_height="match_parent">



    <TextView
        android:textAlignment="center"
        android:textColor="@color/white"

        android:textSize="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/br1"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:text="Ljubicasti" />

    <TextView


        android:textColor="@color/white"
        android:textSize="20dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:id="@+id/textView17"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:textAlignment="center"
        android:text="Crveni" />

    <EditText
        android:layout_marginLeft="120dp"
        android:layout_marginTop="10dp"
        android:id="@+id/editTextText4"
        android:layout_width="50dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="text"
        android:text="bod"
        android:background="@color/white"/>

    <EditText
        android:layout_marginRight="120dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:id="@+id/editTextText7"
        android:layout_width="50dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="text"
        android:text="bod" />

    <TextView
        android:textColor="@color/white"
        android:id="@+id/textView15"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:text="304"
        android:textAlignment="center"
        android:layout_below="@id/br1"
        android:layout_marginTop="40dp"
        android:layout_centerHorizontal="true"
        android:textSize="20dp"
        />

    <LinearLayout
        android:layout_centerInParent="true"
        android:id="@+id/prvilin"
        android:layout_width="349dp"
        android:layout_height="37dp"
        android:layout_below="@id/textView15"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:textAlignment="center">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:background="@color/white"
            android:text="3"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:background="@color/white"
            android:text="4"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:background="@color/white"
            android:text="5"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:background="@color/white"
            android:text="8"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:background="@color/white"
            android:text="20"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:background="@color/white"
            android:text="100"
            android:textAlignment="center" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/drugilin"
        android:layout_width="349dp"
        android:layout_height="47dp"
        android:layout_below="@id/prvilin"
        android:layout_centerInParent="true">

        <Button
            android:id="@+id/button4"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_below="@id/prvilin"
            android:layout_marginEnd="8dp"
            android:text="+" />

        <Button
            android:id="@+id/button5"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_below="@id/prvilin"
            android:layout_marginEnd="8dp"


            android:text="-" />

        <Button
            android:id="@+id/button6"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_below="@id/prvilin"
            android:layout_marginEnd="8dp"
            android:text="/" />

        <Button
            android:id="@+id/button7"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_below="@id/prvilin"
            android:layout_marginEnd="8dp"
            android:text=")" />

        <Button
            android:id="@+id/button8"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_below="@id/prvilin"
            android:layout_marginEnd="8dp"
            android:text="("
            android:minHeight="48dp"
            android:minWidth="48dp"/>

        <Button
            android:id="@+id/button9"
            android:layout_width="50dp"
            android:layout_height="48dp"
            android:layout_below="@id/prvilin"
            android:layout_marginEnd="8dp"
            android:text="*" />

    </LinearLayout>

    <EditText
        android:id="@+id/editTextText9"
        android:layout_width="304dp"
        android:layout_height="48dp"
        android:layout_below="@id/drugilin"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:background="@color/white"
        android:ems="10"
        android:inputType="text"
        android:text="Unesi resenje"
        android:textAlignment="center" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="U redu"
        android:layout_below="@id/editTextText9"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"/>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/editTextText9"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:background="@color/white"
        android:text="Time"
        android:textAlignment="center"
        android:textSize="25dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stop"
        android:layout_below="@id/editTextText9"
        android:layout_marginTop="5dp"/>


</android.widget.RelativeLayout>